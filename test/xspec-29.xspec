<?xml version="1.0" encoding="UTF-8"?>
<x:description stylesheet="xspec-29.xsl" xmlns:x="http://www.jenitennison.com/xslt/xspec">
	<x:scenario label="x:context has an element with an attribute containing curly bracket">
		<x:context>
			<file>
				<element
					path="/Q{http://www.w3.org/1999/XSL/Transform}stylesheet[1]/Q{http://www.w3.org/1999/XSL/Transform}template[1]"
				 />
			</file>
		</x:context>
		<x:expect label="x:expect with the same element">
			<file>
				<element
					path="/Q{http://www.w3.org/1999/XSL/Transform}stylesheet[1]/Q{http://www.w3.org/1999/XSL/Transform}template[1]"
				 />
			</file>
		</x:expect>
		<x:expect label="x:expect with xs:string @select"
			select="'/Q{http://www.w3.org/1999/XSL/Transform}stylesheet[1]/Q{http://www.w3.org/1999/XSL/Transform}template[1]'"
			test="string(file/element/@path)" />
		<x:expect label="x:expect with string-length @test" select="104"
			test="string-length(file/element/@path)" />
		<x:expect label="x:expect with xs:boolean @test"
			test="string(file/element/@path) eq '/Q{http://www.w3.org/1999/XSL/Transform}stylesheet[1]/Q{http://www.w3.org/1999/XSL/Transform}template[1]'"
		 />
	</x:scenario>
</x:description>
