<?xml version="1.0" encoding="UTF-8"?>
<?xspec-test message=ERROR in x:call (under 'Naming a static library option in x:call/x:option'): Cannot specify option 's:add-attr' because it is static.?>
<x:description xmlns:s="x-urn:test:xproc:steplibrary"
    xmlns:x="http://www.jenitennison.com/xslt/xspec" xproc="../cases/library-static-options.xpl">
    <x:scenario label="Naming a static library option in x:call/x:option">
        <x:call step="s:add-attrs-maybe">
            <x:option name="s:add-attr" select="true()"/>
        </x:call>
        <x:expect label="Raises an error" port="result">
            <document global="1" local="1"/>
        </x:expect>
    </x:scenario>
</x:description>
