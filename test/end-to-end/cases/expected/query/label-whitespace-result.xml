   <scenario id="scenario1" xspec="../../label-whitespace.xspec">
      <label>x:like label with untrimmed whitespace</label>
      <input-wrap xmlns="">
         <x:call xmlns:x="http://www.jenitennison.com/xslt/xspec" function="string">
            <x:param select="'untrimmed'"/>
         </x:call>
      </input-wrap>
      <scenario id="scenario1-scenario1" xspec="../../label-whitespace.xspec">
         <label>in x:like/@label targeting x:scenario/@label</label>
         <result select="'untrimmed'"/>
         <test id="scenario1-scenario1-expect1" successful="true">
            <label>matches scenario @label with untrimmed whitespace</label>
            <expect select="'untrimmed'"/>
         </test>
