         </x:param>
      </x:call>
      <x:result select="/element()">
         <e1 xmlns:mirror="x-urn:test:mirror" xmlns="ns1">
            <e2 xmlns:ns3="ns3" xmlns:ns4="ns4" xmlns="ns2">
